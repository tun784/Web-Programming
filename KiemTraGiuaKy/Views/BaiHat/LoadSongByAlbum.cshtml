@model IEnumerable<KiemTraGiuaKy.Models.BaiHat>
@{
    ViewBag.Title = "LoadSongByAlbum";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MaBH)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TenBH)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaTL)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MaNS)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.MaBH)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TenBH)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaTL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MaNS)
            </td>
            <td>
                @Html.ActionLink("Delete from Album", "DeleteSongFromAlbum", "BaiHat", new { idSong = item.MaBH, idAB = ViewBag.IdAB }, null)
            </td>
        </tr>
    }

</table>
